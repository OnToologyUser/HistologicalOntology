<http://www.univalle.edu.co/ontologies/organ#OrganoArteria>
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass <http://www.univalle.edu.co/ontologies/organ#SistemaCirculatorioSanguineo> ;
                owl:onProperty <http://www.univalle.edu.co/ontologies/organ#esOrganoDe> ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      owl:equivalentClass
              [ a       owl:Restriction ;
                owl:onProperty <http://www.semanticweb.org/mmvlab/ontologies/2015/2/System#tieneRegionDeLuz> ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:intersectionOf (<http://www.univalle.edu.co/ontologies/Tissue#EstructuraRegionLuz> [ a       owl:Restriction ;
                                      owl:hasValue "Angosta" ;
                                      owl:onProperty <http://www.semanticweb.org/mmvlab/ontologies/2015/2/System#amplitud>
                                    ])
                        ]
              ] .

<http://www.semanticweb.org/mmvlab/ontologies/2015/2/System#tieneRegionDeLuz>
